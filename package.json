{
    "name": "rainbow_csv",
    "displayName": "rainbow_csv",
    "description": "rainbow_csv: highlight csv/tsv files and execute SQL queries",
    "version": "0.1",
    "publisher": "Dmitry Ignatovich",
    "engines": {
        "vscode": "^1.19.0"
    },
    "keywords": [
        "csv",
        "tsv",
        "highlight"
    ],
    "categories": [
        "Other",
        "Languages"
    ],
    "activationEvents": [
        "onLanguage:csv", "onLanguage:tsv"
    ],
    "main": "./extension",
    "contributes": {
        "languages": [{
            "id": "csv",
            "extensions": [".csv"]
        },
        {
            "id": "tsv",
            "extensions": [".tsv"]
        }],
        "grammars": [{
            "language": "csv",
            "scopeName": "text.csv",
            "path": "./syntaxes/csv.tmLanguage.json"
        },
        {
            "language": "tsv",
            "scopeName": "text.tsv",
            "path": "./syntaxes/tsv.tmLanguage.json"
        }]
    },
    "capabilities": {
        "hoverProvider": "true"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
