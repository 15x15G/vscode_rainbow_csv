{
    "name": "rainbow-csv",
    "displayName": "Rainbow CSV",
    "description": "Rainbow CSV: highlight csv and tsv files in different colors",
    "version": "0.1.0",
    "publisher": "mechatroner",
    "icon": "rainbow_csv_logo.png",
    "engines": {
        "vscode": "^1.19.0"
    },
    "keywords": [
        "csv",
        "tsv",
        "highlight"
    ],
    "categories": [
        "Other",
        "Languages"
    ],
    "activationEvents": [
        "onLanguage:CSV", "onLanguage:TSV", "onLanguage:CSV (semicolon)"
    ],
    "main": "./extension",
    "contributes": {
        "languages": [{
            "id": "CSV",
            "extensions": [".csv"]
        },
        {
            "id": "TSV",
            "extensions": [".tsv"]
        },
        {
            "id": "CSV (semicolon)",
            "extensions": [".scsv"]
        }],
        "commands": [{
            "command": "extension.CSVLint",
            "title": "CSV Lint"
        }],
        "grammars": [{
            "language": "CSV",
            "scopeName": "text.csv",
            "path": "./syntaxes/csv.tmLanguage.json"
        },
        {
            "language": "TSV",
            "scopeName": "text.tsv",
            "path": "./syntaxes/tsv.tmLanguage.json"
        },
        {
            "language": "CSV (semicolon)",
            "scopeName": "text.scsv",
            "path": "./syntaxes/scsv.tmLanguage.json"
        }]
    },
    "capabilities": {
        "hoverProvider": "true"
    },
    "devDependencies": {
        "vscode": "^1.1.6"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mechatroner/vscode_rainbow_csv.git"
    }
}
