{
    "name": "rainbow_csv",
    "displayName": "rainbow_csv",
    "description": "rainbow_csv: highlight csv/tsv files and execute SQL queries",
    "version": "0.0.1",
    "publisher": "name",
    "engines": {
        "vscode": "^1.19.0"
    },
    "keywords": [
        "csv",
        "tsv",
        "highlight",
        "sql"
    ],
    "categories": [
        "Other",
        "Languages"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.sayHello",
            "title": "Hello World"
        }],
        "languages": [{
            "id": "csv",
            "extensions": [".csv"]
        },
        {
            "id": "tsv",
            "extensions": [".tsv"]
        }],
        "grammars": [{
            "language": "csv",
            "scopeName": "text.csv",
            "path": "./syntaxes/csv.tmLanguage.json"
        },
        {
            "language": "tsv",
            "scopeName": "text.tsv",
            "path": "./syntaxes/tsv.tmLanguage.json"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
